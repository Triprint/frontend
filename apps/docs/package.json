{
  "name": "@triprint-ui/docs",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "start-storybook --no-open --port=6006",
    "build": "build-storybook",
    "start": "http-server storybook-static",
    "lint": "TIMING=1 eslint \".storybook/**/*.js\" \"stories/**/*.{ts,tsx,md,mdx}\"",
    "format": "prettier --write \".storybook/**/*.js\" \"stories/**/*.{ts,tsx,md,mdx}\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf storybook-static"
  },
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "@triprint-ui/components": "*",
    "@triprint-ui/core": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.14",
    "@triprint/tsconfig": "*",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint-config-triprint": "*",
    "http-server": "^14.1.1",
    "typescript": "4.9.4",
    "vite": "^4.0.1"
  }
}
