{
  "private": true,
  "name": "triprint",
  "description": "TRIPRINT",
  "author": "sangrimlee <contact@sangrim.dev>",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "configs/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "start:admin": "turbo run start --filter=admin",
    "start:client": "turbo run start --filter=client",
    "start:docs": "turbo run start --filter=docs",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "@triprint/eslint-config": "*",
    "@triprint/prettier-config": "*",
    "eslint": "^8.29.0",
    "prettier": "^2.8.1",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@1.22.19"
}
