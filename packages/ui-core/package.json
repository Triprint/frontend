    "eslint": "^8.29.0",
    "eslint-config-triprint": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.7.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.4.1",
    "typescript": "4.7.4"
