{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "storybook-static/**"],
      "cache": true
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "cache": true
    },
    "format": {
      "cache": true
    },
    "clean": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  }
}
